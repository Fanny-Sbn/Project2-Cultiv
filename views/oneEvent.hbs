<div id="content-all">
    
{{#each this}}
<div id="test">
<h1>{{this.title}}</h1> <p>{{this.category}}</p> 
</div>
<img id="imgEvent" src="{{this.cover_url}}" alt=""></img>
{{/each}}



<div id="description-event">
{{#each this}}
<p>{{this.lead_text}}</p>
<p>{{{this.description}}}</p>
{{/each}}
</div>

<div id="info-pratiques">
<h2>INFOS PRATIQUES</h2>
<h3>Quand :</h3>
{{#each this}}
<p>{{{this.date_description}}}</p> 
{{/each}}

<h3>Où :</h3>
{{#each this}}
<p>{{this.address_name}}</p>
<p>{{this.address_street}} {{this.address_zipcode}} {{this.address_city}}</p> 
<p>{{{this.transport}}}</p> 
<p>{{contact_phone}}</p> 
{{/each}}

<h3>Prix :</h3>
{{#each this}}
<p>{{this.price_type}}</p>
<p>{{this.price_detail}}</p>
{{/each}}

<h3>Réseaux sociaux :</h3>
<a href="/add-favorite"><i class="fab fa-twitter"></i></a>
<a href="{{contact_facebook}}"><i class="fab fa-facebook-f"></i></a>
</div>

</div>

